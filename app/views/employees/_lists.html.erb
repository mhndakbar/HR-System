<p>----------------</p>
<h4>List of divisions</h4>
<div>
  <ul>
    <% @divisions.each do |division| %>
        <li>
          <%= division.id %><span> </span><%= division.name %>
        </li>
      <% end %>
   </ul>
</div>
<h4>List of teams</h4>
<div>
  <ul>
    <% @teams.each do |team| %>
        <li>
          <%= team.id %><span> </span><%= team.name %>
        </li>
      <% end %>
   </ul>
</div>